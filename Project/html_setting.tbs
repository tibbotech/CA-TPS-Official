public sub proc_html_io_event()
	dim s1,s2 as string(16)
	dim s3 as string(NUM_OF_IO*3)
	dim i as byte
	dim pos1, pos2 as byte
	
	pos1=instr(1,sock.httprqstring,"i=",1)
	pos2=instr(1,sock.httprqstring,"z=",1)

	if pos1=0 and pos2=0 then
		'no item found, it's not a click triggered event, return all status
		s3=""
		for i=0 to NUM_OF_IO-1
			if is_io_active(i)=YES then
				io.num=index_to_io_num(i)
				if io.state=LOW then
					s3=s3+"0"
				else
					s3=s3+"1"
				end if
			else
				s3=s3+"2"
			end if
		next i

		for i=0 to NUM_OF_IO-1
			io.num=index_to_io_num(i)
			if io.enabled=yes then
				s3=s3+"1"
			else
				s3=s3+"0"
			end if			
		next i
		
		for i=0 to NUM_OF_IO-1
			if param_io(i).io_output=1 then
				s3=s3+"1"
			else
				s3=s3+"0"
			end if			
		next i
		
		sock_setsend(s3)
		exit sub	
	end if
	
	login_timeout_reset()
	if pos1>0 then
		s1=get_http_argument(sock.httprqstring,"i=")
		s2=left(s1,1)
		select case s2
		case "a":
			s2=right(s1,len(s1)-1)
			s1=get_http_argument(sock.httprqstring,"s=")
			stg_set("I"+s2,0,s1)
			i=val(s2)
			if is_io_active(i)=YES then
				io.num=index_to_io_num(i)
				if io.state=LOW then
					s1="0"
				else
					s1="1"
				end if
				sock_setsend(s1)
			else
				s1="2"
			end if
			
		case "b":
			
			s2=right(s1,len(s1)-1)
			s1=get_http_argument(sock.httprqstring,"s=")
			stg_set("I"+s2,1,s1)
			i=val(s2)
			if is_io_active(i)=YES then
				io.num=index_to_io_num(i)
				if io.state=LOW then
					s1="0"
				else
					s1="1"
				end if
				sock_setsend(s1)
			else
				s1="2"
			end if
			
		case "K":
			s2=right(s1,len(s1)-1)
			s1=get_http_argument(sock.httprqstring,"s=")
		
		#if PLATFORM_ID=TPP3W or PLATFORM_ID=TPP3WG2 or PLATFORM_ID=TPP3N
			i=val(s2)/4
			
			select case s2
			case "1","5","9","13":
				select case s1
				case "0": 
					stg_set("SE",i,"0")
					stg_set("FC",i,"0")
				case "1":
					stg_set("SE",i,"1")
					stg_set("FC",i,"0")			
				case "2":
					stg_set("SE",i,"1")
					stg_set("FC",i,"1")	
				case else:
					
				end select
			case "3","7","11","15","17","19","21","23","25","27":
			case else:
			end select
		#endif
		
		#if PLATFORM_ID=TPP2W or PLATFORM_ID=TPP2WG2 or PLATFORM_ID=TPP2N
			i=val(s2)/2
			
			select case s2
			case "1","3","5","7":
				select case s1
				case "0": 
					stg_set("SE",i,"0")
					stg_set("FC",i,"0")
				case "1":
					stg_set("SE",i,"1")
					stg_set("FC",i,"0")			
				case "2":
					stg_set("SE",i,"1")
					stg_set("FC",i,"1")	
				case else:
					
				end select
			case "9","11":
			case else:
			end select	
		#endif
		s1=str(param_se(i))+str(param_fc(i))
		sock_setsend(s1)

		case else:
		
		end select	
	end if

end sub

